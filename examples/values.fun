package main

func main = do
    // Strings, which can be added together with `+`.
    print ("go" + "lang")

    // Integers and floats.
    print ("1+1 =", 1 + 1)
    print ("7.0/3.0 =", 7.0 / 3.0)

    // Booleans, with boolean operators as you'd expect.
    print (true && false) // TODO: or should it be True && False
    print (true || false)
    print (not true)
